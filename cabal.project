packages: ./xmonad-setup.cabal
keep-going: true
shared: true
constraints: base >= 4.11,
             directory >= 1.3,
             xmonad >= 0.17,
             xmonad-contrib >= 0.17,
             X11 >= 1.8,
             xmobar >= 0.35,
             xmobar +with_xft
                    +with_utf8
                    +with_iwlib
                    +with_alsa
                    +with_datezone
                    +with_dbus
                    +with_weather
                    +with_threaded
                    +with_rtsopts
